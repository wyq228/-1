.log-list {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  width: 750rpx;
  height: 750rpx;
}
.img1{
  width: 750px;
  height: 300rpx;
}
.message{
  width: 650rpx;
  height: 250rpx;
  position: absolute;
  top: 260rpx;
  background: white;
  padding: 13rpx;
}
.msg{
  width: 650rpx;
  height: 65rpx;
  font-size: 30rpx;
  line-height: 65rpx;
}
.msg>text{
  margin-left: 40rpx;
}
.msg>button{
  width: 100rpx;
  height: 50rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  position: absolute;
  right: 30rpx;
  top: 70rpx;
}
.msg:nth-of-type(2)>button{
  position: absolute;
  right: 30rpx;
  top: 140rpx;
}
.msg:nth-of-type(3)>button{
  position: absolute;
  right: 30rpx;
  top: 210rpx;
}
.item{
  width: 750rpx;
  height: 200rpx;
  display: flex;
  justify-content: space-around;
}
.item>.font{
  width: 250rpx;
  height: 70rpx;
  text-align: center;
}